build:engflow_common --jobs=8
build:engflow_common --define=EXECUTOR=remote
build:engflow_common --disk_cache=
build:engflow_common --experimental_inmemory_dotd_files
build:engflow_common --experimental_inmemory_jdeps_files
build:engflow_common --incompatible_strict_action_env=true
build:engflow_common --remote_timeout=3600
build:engflow_common --spawn_strategy=remote,local

build:engflow_common --action_env=BAZEL_DO_NOT_DETECT_CPP_TOOLCHAIN=1
build:engflow_common --crosstool_top=@engflow_remote_config//cc:toolchain
build:engflow_common --extra_execution_platforms=@engflow_remote_config//config:platform
build:engflow_common --extra_toolchains=@engflow_remote_config//config:cc-toolchain
build:engflow_common --host_java_toolchain=@bazel_tools//tools/jdk:toolchain_java11
build:engflow_common --host_javabase=@engflow_remote_config//java:jdk
build:engflow_common --host_platform=@engflow_remote_config//config:platform
build:engflow_common --java_toolchain=@bazel_tools//tools/jdk:toolchain_java11
build:engflow_common --javabase=@engflow_remote_config//java:jdk
build:engflow_common --platforms=@engflow_remote_config//config:platform

build:without_bytes --experimental_inmemory_jdeps_files
build:without_bytes --experimental_inmemory_dotd_files
build:without_bytes --experimental_remote_download_outputs=minimal

build:engflow --config=engflow_common
build:engflow --remote_executor=grpcs://10.0.0.10:8080
build:engflow --tls_certificate=engflow-ca.crt
build:engflow --bes_backend=grpcs://10.0.0.10:8080
build:engflow --bes_lifecycle_events
build:engflow --bes_results_url=https://10.0.0.10:8080/invocation/
